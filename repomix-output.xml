}

.feature-card:hover {
    transform: translateY(-8px);
    box-shadow: 0 12px 24px rgba(0, 0, 0, 0.08);
}

.feature-icon {
    font-size: 48px;
    margin-bottom: 20px;
}

.feature-title {
    font-size: 21px;
    font-weight: 600;
    margin-bottom: 12px;
}

.feature-text {
    color: var(--apple-gray);
    font-size: 17px;
    line-height: 1.47059;
}

/* Prompts Section */
.prompts-section {
    background: var(--apple-light-gray);
    border-radius: 18px;
    padding: 40px;
    margin-top: 40px;
}

.prompt-box {
    background: white;
    border-radius: 12px;
    padding: 24px;
    margin-bottom: 20px;
    font-family: 'SF Mono', Monaco, 'Courier New', monospace;
    font-size: 14px;
    line-height: 1.5;
    color: var(--apple-blue);
    box-shadow: 0 2px 8px rgba(0, 0, 0, 0.04);
    transition: all 0.3s ease;
}

.prompt-box:hover {
    transform: translateX(8px);
    box-shadow: 0 4px 16px rgba(0, 0, 0, 0.08);
}

/* CTA Section */
.cta-section {
    background: var(--apple-gradient);
    color: white;
    text-align: center;
    padding: 100px 0;
    position: relative;
    overflow: hidden;
}

.cta-section::before {
    content: '';
    position: absolute;
    top: -50%;
    left: -50%;
    width: 200%;
    height: 200%;
    background: radial-gradient(circle, rgba(255, 255, 255, 0.1) 0%, transparent 70%);
    animation: pulse 15s ease-in-out infinite;
}

@keyframes pulse {
    0%, 100% { transform: scale(1); opacity: 0.3; }
    50% { transform: scale(1.2); opacity: 0.1; }
}

.cta-content {
    position: relative;
    z-index: 1;
}

.cta-headline {
    font-size: 48px;
