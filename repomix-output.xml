}

.accordion-icon {
    font-size: 20px;
    color: var(--apple-gray);
    transition: transform 0.3s ease;
}

.accordion-item.active .accordion-icon {
    transform: rotate(45deg);
}

.accordion-content {
    max-height: 0;
    overflow: hidden;
    transition: max-height 0.3s ease;
}

.accordion-item.active .accordion-content {
    max-height: 1000px;
    padding-bottom: 24px;
}

.accordion-body {
    padding-top: 16px;
    color: var(--apple-gray);
    line-height: 1.58824;
}

/* Feature Cards */
.feature-cards {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
    gap: 20px;
    margin-top: 60px;
}

.feature-card {
    text-align: center;
    padding: 40px 20px;
    background: white;
    border-radius: 18px;
    box-shadow: 0 2px 8px rgba(0, 0, 0, 0.04);
