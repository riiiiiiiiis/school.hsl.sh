    font-size: 48px;
    line-height: 1.08349;
    font-weight: 600;
    letter-spacing: -0.003em;
    margin-bottom: 56px;
}

/* Cards */
.cards-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
    gap: 30px;
    margin-bottom: 80px;
}

.card {
    background: var(--apple-light-gray);
    border-radius: 18px;
    padding: 40px 30px;
    transition: all 0.3s ease;
    position: relative;
    overflow: hidden;
}

.card::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    height: 4px;
    background: var(--apple-gradient);
    transform: translateY(-100%);
    transition: transform 0.3s ease;
}

.card:hover {
    transform: translateY(-4px);
    box-shadow: var(--apple-shadow);
}

.card:hover::before {
    transform: translateY(0);
}

.card-number {
    font-size: 48px;
    font-weight: 600;
    color: var(--apple-gray);
    opacity: 0.3;
    margin-bottom: 16px;
}

.card-title {
    font-size: 24px;
