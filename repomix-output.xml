    margin-bottom: 16px;
    color: var(--apple-black);
}

.card-text {
    font-size: 17px;
    line-height: 1.47059;
    color: var(--apple-gray);
}

/* Program Accordion */
.accordion {
    max-width: 800px;
    margin: 0 auto;
}

.accordion-item {
    border-bottom: 1px solid #d2d2d7;
    padding: 24px 0;
}

.accordion-header {
    display: flex;
    justify-content: space-between;
    align-items: center;
    cursor: pointer;
    padding: 16px 0;
    transition: all 0.3s ease;
}

.accordion-header:hover {
