    transition: color 0.3s ease;
}

.footer-links a:hover {
    color: var(--apple-blue);
}

.footer-bottom {
    padding-top: 40px;
    border-top: 1px solid #d2d2d7;
    text-align: center;
    color: var(--apple-gray);
}

/* Responsive */
@media (max-width: 768px) {
    .nav-menu {
        display: none;
    }

    .hero-headline {
        font-size: 40px;
    }

    .hero-subheadline {
        font-size: 21px;
    }

    .section-headline {
        font-size: 32px;
    }

    .cards-grid {
        grid-template-columns: 1fr;
    }
}
